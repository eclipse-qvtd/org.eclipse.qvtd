<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
    <extension
      point="org.eclipse.ant.core.antTasks">
        <antTask
          class="org.eclipse.qvtd.build.etl.ant.PivotSetup"
          library="org.eclipse.qvtd.build.ant.jar"
          name="PivotSetup">
        </antTask>
    </extension>
    <extension
      point="org.eclipse.epsilon.common.dt.tool">
        <tool
          class="org.eclipse.qvtd.build.etl.PivotUtil"
          defaultName="PivotUtil">
        </tool>
   </extension>
   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated QVTScheduling -->
      <package
            uri="http://www.eclipse.org/qvt/0.1/QVTscheduling/"
            class="org.eclipse.qvtd.build.etl.qvtscheduling.QvtSchedulingPackage"
            genModel="model/QVTScheduling.genmodel"/>
   </extension>

</plugin>
