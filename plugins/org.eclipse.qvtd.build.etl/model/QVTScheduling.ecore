<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="qvtscheduling" nsURI="http://www.eclipse.org/qvt/0.1/QVTscheduling/"
    nsPrefix="qvtscheduling">
  <eClassifiers xsi:type="ecore:EClass" name="AbstractDatum">
    <eStructuralFeatures xsi:type="ecore:EReference" name="sources" upperBound="-1"
        eType="#//AbstractAction" eOpposite="#//AbstractAction/results"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="primaryDistinctData" eType="#//DistinctData"
        eOpposite="#//DistinctData/primaryArgument"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="secondaryDistinctData"
        upperBound="-1" eType="#//DistinctData" eOpposite="#//DistinctData/secondaryArgument"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="parent" eType="#//AbstractDatum"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="super" upperBound="-1"
        eType="#//AbstractDatum"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="type" lowerBound="1" eType="ecore:EClass ../../org.eclipse.ocl.examples.pivot/model/Pivot.ecore#//Type"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="domain" eType="ecore:EClass ../../org.eclipse.qvtd.pivot.qvtcorebase/model/QVTcoreBase.ecore#//CoreDomain"
        derived="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="targets" upperBound="-1"
        eType="#//AbstractAction" derived="true" eOpposite="#//AbstractAction/arguments"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AbstractAction">
    <eStructuralFeatures xsi:type="ecore:EReference" name="results" upperBound="-1"
        eType="#//AbstractDatum" eOpposite="#//AbstractDatum/sources"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="distinctArguments" upperBound="-1"
        eType="#//DistinctData" eOpposite="#//DistinctData/targets"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="count" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="parent" eType="#//AbstractAction"
        eOpposite="#//AbstractAction/children"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="children" upperBound="-1"
        eType="#//AbstractAction" eOpposite="#//AbstractAction/parent"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="arguments" upperBound="-1"
        eType="#//AbstractDatum" derived="true" eOpposite="#//AbstractDatum/targets"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ClassDatum" eSuperTypes="#//AbstractDatum">
    <eStructuralFeatures xsi:type="ecore:EReference" name="realizedVariables" upperBound="-1"
        eType="ecore:EClass ../../org.eclipse.qvtd.pivot.qvtcorebase/model/QVTcoreBase.ecore#//RealizedVariable"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="variables" upperBound="-1"
        eType="ecore:EClass ../../org.eclipse.ocl.examples.pivot/model/Pivot.ecore#//Variable"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DataTypeDatum" eSuperTypes="#//AbstractDatum"/>
  <eClassifiers xsi:type="ecore:EClass" name="DistinctData">
    <eStructuralFeatures xsi:type="ecore:EReference" name="targets" upperBound="-1"
        eType="#//AbstractAction" eOpposite="#//AbstractAction/distinctArguments"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="primaryArgument" upperBound="-1"
        eType="#//AbstractDatum" eOpposite="#//AbstractDatum/primaryDistinctData"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="secondaryArgument" upperBound="-1"
        eType="#//AbstractDatum" eOpposite="#//AbstractDatum/secondaryDistinctData"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="InputAction" eSuperTypes="#//AbstractAction"/>
  <eClassifiers xsi:type="ecore:EClass" name="MappingAction" eSuperTypes="#//AbstractAction">
    <eStructuralFeatures xsi:type="ecore:EReference" name="mappingBindings" upperBound="-1"
        eType="#//MappingBinding" containment="true" eOpposite="#//MappingBinding/mappingAction"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="MappingBinding">
    <eStructuralFeatures xsi:type="ecore:EReference" name="mappingAction" eType="#//MappingAction"
        eOpposite="#//MappingAction/mappingBindings"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="loop" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="datum" eType="#//DataTypeDatum"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OutputAction" eSuperTypes="#//AbstractAction"/>
</ecore:EPackage>
