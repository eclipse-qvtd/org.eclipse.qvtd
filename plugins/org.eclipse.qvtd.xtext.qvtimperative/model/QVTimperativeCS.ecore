    <eStructuralFeatures xsi:type="ecore:EReference" name="queries" upperBound="-1"
        eType="ecore:EClass ../../org.eclipse.qvtd.xtext.qvtcorebase/model/QVTcoreBaseCS.ecore#//QueryCS"
