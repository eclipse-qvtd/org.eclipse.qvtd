module org.eclipse.qvtd.build.cs2as.mwe.GenerateCGedQVTiTransformation

import org.eclipse.qvtd.build.cs2as.mwe.*
import org.eclipse.emf.ecore.resource.*


/**
 * The OCL document URI corresponding to the CS2AS description 
 */
var String oclFileURI

/**
 * Mandatory resource set 
 */
var ResourceSet resourceSet

var String environmentClassName

var String lookupVisitorClassName

var String namedElementItfName

/**
 * Optional output 
 */
var javaFolder = "src-gen/"

var javaPackage = ""



Workflow {

	component = GenerateCGedQVTiTransformation {
		resourceSet = resourceSet
		oclDocURI = oclFileURI
		javaFolder = javaFolder
		javaPackage = javaPackage
		environmentClassName = environmentClassName
		lookupVisitorClassName = lookupVisitorClassName
		namedElementItfName = namedElementItfName 
	}
}
