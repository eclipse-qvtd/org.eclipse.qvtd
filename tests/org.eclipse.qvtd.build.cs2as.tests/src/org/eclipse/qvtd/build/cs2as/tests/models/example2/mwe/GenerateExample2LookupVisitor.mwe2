module org.eclipse.qvtd.build.cs2as.tests.GenerateExample2LookupVisitor

import org.eclipse.ocl.examples.build.*
import org.eclipse.ocl.examples.build.utilities.*
import org.eclipse.ocl.examples.build.xtend.*
import org.eclipse.emf.mwe.utils.*
import org.eclipse.emf.mwe.utils.*
import org.eclipse.emf.ecore.resource.impl.*


var projectName = "org.eclipse.qvtd.build.cs2as.tests"
var examplesFolder = "src/org/eclipse/qvtd/build/cs2as/tests/models/example2"

Workflow {
	bean = ResourceSetImpl : resourceSet {}
    bean = StandaloneSetup { resourceSet = resourceSet
    	platformUri = ".."
    	scanClassPath = true
    }
    bean = GenModelSetup { resourceSet = resourceSet }

    component = @SubGenerateAutoASModel { resourceSet = resourceSet
    	projectName = "${projectName}"
    	modelName = "classes"
    	projectPrefix = "Classes"
    	superProjectName = ""
    	superModelName = ""
    	genModelFile = "${examplesFolder}/Classes.genmodel"
    	lookupFilePath = "${examplesFolder}/ClassesNameResolution_LookupVisitor.ocl"
		visitorPackageName = "example2.classes.util"
		visitorClassName = "Visitor"
    	visitableClassName = "Visitable"
    }
       
}
