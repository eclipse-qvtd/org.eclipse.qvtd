<?xml version="1.0" encoding="ASCII"?>
<simplerdbms:Schema
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:simplerdbms="http://www.eclipse.org/qvt/examples/0.1/SimpleRDBMS ../SimpleRDBMS.ecore">
  <tables name="Student"
      kind="base">
    <columns
        name="Student_tid"
        type="NUMBER"
        keys="//@tables.0/@keys.0"/>
    <columns
        name="guardian"
        type="VARCHAR"/>
    <columns
        name="name"
        type="VARCHAR"/>
    <columns
        name="studentNumber"
        type="NUMBER"/>
    <keys name="Student_pk"
        kind="primary"
        column="//@tables.0/@columns.0"/>
  </tables>
  <tables name="Professor"
      kind="base">
    <columns
        name="Professor_Id"
        type="NUMBER"
        keys="//@tables.1/@keys.0"/>
    <columns
        name="employed"
        type="BOOLEAN"/>
    <columns
        name="name"
        type="VARCHAR"/>
    <keys name="Professor_pk"
        kind="primary"
        column="//@tables.1/@columns.0"/>
  </tables>
  <tables name="Address"
      kind="base">
    <columns
        name="Address_Id"
        type="NUMBER"
        keys="//@tables.2/@keys.0"/>
    <keys name="Address_pk"
        column="//@tables.2/@columns.0"/>
  </tables>
  <tables name="Timetable"
      kind="base">
    <columns
        name="Timetable_Id"
        type="NUMBER"
        keys="//@tables.3/@keys.0"/>
    <columns
        name="year"
        type="VARCHAR"/>
    <keys name="Timetable_pk"
        kind="primary"
        column="//@tables.3/@columns.0"/>
  </tables>
  <tables name="ResearchGroup"
      kind="base">
    <columns
        name="ResearchGroup_Id"
        type="NUMBER"
        keys="//@tables.4/@keys.0"/>
    <columns
        name="name"
        type="VARCHAR"/>
    <keys name="ResearchGroup_pk"
        kind="primary"
        column="//@tables.4/@columns.0"/>
  </tables>
  <tables name="Budget"
      kind="base">
    <columns
        name="Budget_Id"
        kind=""
        type="NUMBER"
        keys="//@tables.5/@keys.0"/>
    <columns
        name="year"
        type="VARCHAR"/>
    <keys name="Budget_pk"
        kind="primary"
        column="//@tables.5/@columns.0"/>
  </tables>
</simplerdbms:Schema>
