import myMM : 'Called.ecore'::called; 

package org::eclipse::qvtd::xtext::qvtrelation::tests::callingConventions {
	transformation ExternalFunctionCallingConventionTest(from:myMM, to:myMM)
	{
		query externalSum(firstValue : ecore::EInt[1], secondValue : ecore::EInt[1]) : ecore::EInt[1]
		implementedby 'org.eclipse.qvtd.xtext.qvtrelation.tests.helpers.ExternalFunctionSum';

		top relation Values2Values {
			value : ecore::EInt[1];
			domain from fromValues : Values{ integer = value };
			enforce domain to toValues : Values{ integer = externalSum(value, 1) };
		}
	}
}