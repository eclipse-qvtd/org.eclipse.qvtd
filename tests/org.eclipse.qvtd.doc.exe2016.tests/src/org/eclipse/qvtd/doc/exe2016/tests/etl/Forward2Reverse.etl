rule list2list
	transform
		f : ForwardList!DoublyLinkedList
	to
		r : ReverseList!DoublyLinkedList {
	
	r.name = f.name;
	r.headElement ::= f.headElement;
		
}

rule element2element
	transform
		f : ForwardList!Element
	to
		r : ReverseList!Element {
	r.name = f.name;
	r.list ::= f.list;
	r.source ::= f.target;
	r.target ::= f.source;
}