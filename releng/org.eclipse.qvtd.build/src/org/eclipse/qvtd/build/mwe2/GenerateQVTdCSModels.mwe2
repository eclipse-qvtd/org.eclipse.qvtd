module GenerateQVTdCSModels

import org.eclipse.ocl.examples.build.*
import org.eclipse.ocl.examples.build.utilities.*
import org.eclipse.ocl.examples.build.xtend.*
import org.eclipse.emf.mwe.utils.*
import org.eclipse.emf.ecore.resource.impl.*

/**
 *	Generate the Concrete Syntax Models used by the Xtext editors.
 */
Workflow {
	bean = ResourceSetImpl : resourceSet {}
    bean = StandaloneSetup { resourceSet = resourceSet
    	platformUri = ".."
    	scanClassPath = true
    }
    bean = GenModelSetup { resourceSet = resourceSet }
    bean = UMLGenModelSetup { resourceSet = resourceSet }
    component = @SubGenerateCSModel { resourceSet = resourceSet
    	projectName = "org.eclipse.qvtd.xtext.qvtcorebase"
    	modelName = "qvtcorebasecs"
    	projectPrefix = "QVTcoreBaseCS"
    	superProjectName = "org.eclipse.ocl.xtext.essentialocl"
   		superModelName = "essentialoclcs"
    	superProjectPrefix = "EssentialOCLCS"
    	modelPackageName = "org.eclipse.qvtd.xtext.qvtcorebase.qvtcorebasecs"
    	visitorPackageName = "org.eclipse.qvtd.xtext.qvtcorebase.qvtcorebasecs.util"
    }
    component = @SubGenerateCSModel { resourceSet = resourceSet
    	projectName = "org.eclipse.qvtd.xtext.qvtimperative"
    	modelName = "qvtimperativecs"
    	projectPrefix = "QVTimperativeCS"
    	superProjectName = "org.eclipse.qvtd.xtext.qvtcorebase"
   		superModelName = "qvtcorebasecs"
    	superProjectPrefix = "QVTcoreBaseCS"
    	modelPackageName = "org.eclipse.qvtd.xtext.qvtimperative.qvtimperativecs"
    	visitorPackageName = "org.eclipse.qvtd.xtext.qvtimperative.qvtimperativecs.util"
    	superVisitorPackageName = "org.eclipse.qvtd.xtext.qvtcorebase.qvtcorebasecs.util"
    }
    component = @SubGenerateCSModel { resourceSet = resourceSet
    	projectName = "org.eclipse.qvtd.xtext.qvtcore"
    	modelName = "qvtcorecs"
    	projectPrefix = "QVTcoreCS"
    	superProjectName = "org.eclipse.qvtd.xtext.qvtcorebase"
   		superModelName = "qvtcorebasecs"
    	superProjectPrefix = "QVTcoreBaseCS"
    	modelPackageName = "org.eclipse.qvtd.xtext.qvtcore.qvtcorecs"
    	visitorPackageName = "org.eclipse.qvtd.xtext.qvtcore.qvtcorecs.util"
    	superVisitorPackageName = "org.eclipse.qvtd.xtext.qvtcorebase.qvtcorebasecs.util"
    }
    component = @SubGenerateCSModel { resourceSet = resourceSet
    	projectName = "org.eclipse.qvtd.xtext.qvtrelation"
    	modelName = "qvtrelationcs"
    	projectPrefix = "QVTrelationCS"
    	superProjectName = "org.eclipse.ocl.xtext.essentialocl"
   		superModelName = "essentialoclcs"
    	superProjectPrefix = "EssentialOCLCS"
    	modelPackageName = "org.eclipse.qvtd.xtext.qvtrelation.qvtrelationcs"
    	visitorPackageName = "org.eclipse.qvtd.xtext.qvtrelation.qvtrelationcs.util"
    }
}